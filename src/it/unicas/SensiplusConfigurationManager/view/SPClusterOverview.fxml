<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="395.0" prefWidth="748.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unicas.SensiplusConfigurationManager.view.SPClusterOverviewController">
   <children>
      <TableView fx:id="spClusterTableView" prefHeight="356.0" prefWidth="748.0" AnchorPane.bottomAnchor="39.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="idSPClusterColumn" prefWidth="373.0" text="Id Cluster" />
          <TableColumn fx:id="Name_SPCalibrationColumn" prefWidth="374.0" text="Calibration" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="200.0" layoutY="354.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="1.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleSearchSPCluster" text="Search">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleNewSPCluster" prefHeight="25.0" text="New">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleDeleteSPCluster" text="Delete">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleEditSPCluster" text="Edit">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
